A := {0 .. 5};
B := {1, 3 .. 10};
C:= {2, 4 .. 10};

print("#A = $ #A $");
print("#B = $ #B $");
print("#C = $ #C $");
print("#(A*B) = $ #(A*B) $");
print("#(A*C) = $ #(A*C) $");
print("#(B*C) = $ #(B*C) $");
print("#(A * B* C) = $ #(A * B* C) $");

print("#(A + B + C) = $ #(A + B + C) $");
print("A + B + C = $ A + B + C $");